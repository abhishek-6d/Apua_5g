<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sixdee.imp.dto">
	<class name="AreaLocationTabDTO" table="AREA_LOCATION_DETAILS">
		<id name="id"  type="java.lang.Integer" column="ID">
			<generator class="increment"/> 
		</id>
		<property name="area" type="java.lang.String" column="AREA"/>
		<property name="location" type="java.lang.String" column="LOCATION"/>
	</class>
</hibernate-mapping>