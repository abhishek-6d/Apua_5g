<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class  dynamic-insert="true" name="com.sixdee.imp.dto.MarketingPlanDTO" table="MARKETING_PLAN_MASTER">
		<id name="marketingPlanId" type="integer" column="MARKETING_PLAN_ID">
		</id>
		<property name="marketingPlanName" column="MARKETING_PLAN_NAME"></property>
		<property name="marketingPlanDesc" column="MARKETING_PLAN_DESCRIPTION"></property>
		<property name="marketingPlanObj" column="MARKETING_PLAN_OBJECTIVES"></property>
		<property name="smsKeyword" column="SMS_KEYWORD"></property>
		<property name="dialCode" column="USSD_DIAL_CODE"></property>
	</class>
	
	<class  dynamic-insert="true" name="com.sixdee.imp.dto.UserDTO" table="LOGIN_MASTER">
		<id name="userId" type="integer" column="USER_ID">
		</id>
		<property name="userName" column="USERNAME"></property>
		<property name="password" column="USER_PASSWORD"></property>
		 
	</class>
	
	
</hibernate-mapping>	