<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class  dynamic-insert="true" name="com.sixdee.imp.dto.TierInfoDTO"  table="TIER_INFO">
		<id name="tierId" type="java.lang.Integer" column="ID">
		</id>
		<property name="tierName" column="TIER_NAME"></property>
		<property name="minValue" column="MIN_VALUE"></property>
		<property name="maxValue" column="MAX_VALUE"></property>

		<property name="welcomeRewardPoints" column="WELCOME_REWARD_POINTS"></property>
		<property name="welcomeStatusPoints" column="WELCOME_STATUS_POINTS"></property>
		<property name="tierRewardPoints" column="TIER_REWARD_POINTS"></property>
	</class>
	
	
</hibernate-mapping>	