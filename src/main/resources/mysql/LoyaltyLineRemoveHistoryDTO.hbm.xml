<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class  dynamic-insert="true" name="com.sixdee.imp.simulator.LoyaltyLineRemoveHistoryDTO" table="LOYALTY_LINE_REMOVED_HISTORY">
		<id name="id" type="java.lang.Long" column="ID">
		<generator class="native">
            </generator>
		</id>
		
		<property name="loyaltyId" column="LOYALTY_ID"></property>
		<property name="lineNumber" column="LINE_NUMBER"></property>
		<property name="accountNumber" column="ACCOUNT_NO"></property>
		<property name="accountType" column="ACCOUNT_TYPE"></property>
		<property name="accountCategory" column="ACCOUNT_CATEGORY"></property>
		<property name="lineRewardPoints" column="LINE_REWARD_POINTS"></property>
		<property name="accountRewardPoints" column="ACCOUNT_REWARD_POINTS"></property>
		<property name="lineStatusPoints" column="LINE_STATUS_POINTS"></property>
		<property name="lineAddedDate" column="LINE_ADDED_DATE"></property>
	</class>
	
</hibernate-mapping>	